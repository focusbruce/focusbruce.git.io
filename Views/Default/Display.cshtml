<div class="tanchu_frame" id="loadUpdate" style="display: none;">
    <input type="hidden" id="_UpdateId" />
    <div class="tanchu">
        <table border="0" cellspacing="0" cellpadding="0" class="xunjia_main">
            <tr>
                <td align="right">
                    用户名:
                </td>
                <td align="left" style="padding-left: 10px;">
                    <input type="text" class="xunjie_title" id="_UpdateCode" value="" style="width:160px;">
                </td>
                <td align="right">
                    密码:
                </td>
                <td align="left" style="padding-left: 10px;">
                    <input type="text" class="xunjie_title" id="_UpdatePwd" value="" style="width:160px;">
                </td>
            </tr>
            <tr>
                <td align="right">
                    密保问题:
                </td>
                <td align="left" style="padding-left: 10px;">
                    <input type="text" class="xunjie_title" id="_UpdateProblem" value="" style="width:160px;">
                </td>
                <td align="right">
                    密保答案:
                </td>
                <td align="left" style="padding-left: 10px;">
                    <input type="text" class="xunjie_title" id="_UpdateAnswer" value="" style="width:160px;">
                </td>
            </tr>
            <tr>
                <td align="right">
                    QQ群名:
                </td>
                <td align="left" style="padding-left: 10px;">
                    <input type="text" class="xunjie_title" id="_UpdateQQCrowdName" value="" style="width:160px;">
                </td>
                <td align="right">
                    QQ号:
                </td>
                <td align="left" style="padding-left: 10px;">
                    <input type="text" class="xunjie_title" id="_UpdateQQCode" value="" style="width:160px;">
                </td>
            </tr>
            <tr>
                <td align="right">
                    姓名:
                </td>
                <td align="left" style="padding-left: 10px;">
                    <input type="text" class="xunjie_title" id="_UpdateName" value="" style="width:160px;">
                </td>
                <td align="right">
