@model HLSJ.Web.Models.UpModel
@{
    ViewBag.Title = "充值记录";
    Layout = "~/Views/Shared/_LayoutBefore.cshtml";
}
<script src="~/Content/Js/JsMyGold.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        onPageLoad();
    });
</script>
<div class="fr main">
    <div class="box">
        <div class="boxtitle">充值记录</div>
        <div class="boxinfo" style="min-height: 470px; _height: 470px;">
            <div>
                <table class="border" id="Egv" style="border-collapse: collapse;" cellpadding="0" cellspacing="0" border="0">
                    <tr style="height: 30px;" align="center">
                        <th scope="col" style="width: 60px; cursor: col-resize;">
                            <div style="width: 99%; line-height: 24px; cursor: default;">序号</div>
                        </th>
                        <th style="cursor: col-resize;" scope="col">
                            <div style="width: 99%; line-height: 24px; cursor: default;">金币数量</div>
                        </th>
                        <th scope="col" style="width: 150px; cursor: col-resize;">
                            <div style="width: 99%; line-height: 24px; cursor: default;">充值状态</div>
                        </th>
                        <th scope="col" style="width: 150px; cursor: col-resize;">
                            <div style="width: 99%; line-height: 24px; cursor: default;">充值日期</div>
                        </th>
                    </tr>
                    <tbody id="tbList"></tbody>
                </table>
                <div id="divPager" class="dfPager">
                </div>
            </div>
        </div>
    </div>
</div>
